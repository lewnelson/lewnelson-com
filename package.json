{
  "name": "lewnelson-com",
  "version": "1.0.0",
  "description": "lewnelson.com",
  "main": "index.js",
  "scripts": {
    "postinstall": "./scripts/post-install.sh",
    "start": "nodemon --ignore './src' ./server.js",
    "dev": "npm-run-all --parallel livereload start build:dev",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode none --watch --env.NODE_ENV=development",
    "livereload": "node ./liveReload.js",
    "test": "TZ='UTC' jest",
    "lint": "eslint --ext .js src/",
    "coverage": "TZ='UTC' jest --coverage",
    "coverage:ci": "npm run coverage -- --runInBand --forceExit",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewnelson/lewnelson-com.git"
  },
  "author": "Lewis Nelson <lewis@lewnelson.com> (https://lewnelson.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lewnelson/lewnelson-com/issues"
  },
  "homepage": "https://github.com/lewnelson/lewnelson-com#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jestSetup.js",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.js?(x)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.1.0",
    "modernizr": "^3.6.0",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^7.0.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "ws": "^5.2.0"
  },
  "dependencies": {
    "base64-inline-loader": "^1.1.1",
    "compression": "^1.7.2",
    "connected-react-router": "^4.3.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "matter-js": "^0.14.2",
    "normalize-scss": "^7.0.1",
    "oimo": "^1.0.9",
    "postprocessing": "^4.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "three": "^0.93.0",
    "three-shader-fxaa": "^5.1.1",
    "uuid": "^3.2.1"
  }
}
